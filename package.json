{
  "name": "sorpresa",
  "version": "1.0.0",
  "description": "PLwG 2nd Project | 2025",
  "main": "server.js",
  "type": "module",
  "db-migrate": {
    "migrationsDir": "migrations"
  },
  "scripts": {
    "start": "nodemon dist/server.js",
    "dev": "ts-node src/server.ts",
    "format": "prettier . --write",
    "build": "tsc",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "db:migrate:up": "db-migrate up --config database.cjs --env dev --verbose --migrations-dir ./migrations",
    "db:migrate:down": "db-migrate down --config database.cjs --env dev --verbose --migrations-dir ./migrations",
    "db:seed": "node dist/database/seed.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Janaherself/sorpresa.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Janaherself/sorpresa/issues"
  },
  "homepage": "https://github.com/Janaherself/sorpresa#readme",
  "keywords": [],
  "dependencies": {
    "bcrypt": "^6.0.0",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.14",
    "db-migrate-pg": "^1.5.2",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.11",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^24.7.1",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.3",
    "cross-env": "^10.1.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
